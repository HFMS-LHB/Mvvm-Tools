<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:converters="clr-namespace:MvvmTools.Converters">

    <system:Boolean x:Key="True">True</system:Boolean>
    <system:Boolean x:Key="False">False</system:Boolean>

    <converters:RadioButtonCheckedConverter x:Key="RadioButtonCheckedConverter" />
    <converters:NullToVisibleConverter x:Key="NullToVisibleConverter" />
    <converters:NullToCollapsedConverter x:Key="NullToCollapsedConverter" />
    <converters:FalseToVisibleConverter x:Key="FalseToVisibleConverter" />
    <converters:BoolInvertedConverter x:Key="BoolInvertedConverter" />
    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <converters:ProjectDisplayNameConverter x:Key="ProjectDisplayNameConverter" />
    <converters:ZeroToCollapsedConverter x:Key="ZeroToCollapsedConverter" />
    <converters:EmptyToCollapsedConverter x:Key="EmptyToCollapsedConverter" />
    <converters:EmptyStringConverter x:Key="EmptyStringConverter"/>

</ResourceDictionary>